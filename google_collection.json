											"\r",
											"//récupération d'un identifiant de livre pour tester la recherche d'ouvrage unique\r",
											"const bookId= pm.response.json().items[0].id\r",
											"pm.collectionVariables.set(\"bookId\", bookId)\r",
											"\r",
											"\r",
											"pm.test(\"Le code de statut en égal à 200\", function () {\r",
											"    pm.response.to.have.status(200);\r",
											"});\r",
											"\r",
											"   \r",
											"pm.test(\"Tous les items de la liste ont dans leur titre ou leur sous-titre une chaîne de caractères correspondant à celle envoyée dans la requête\", function(){\r",
											"\r",
											"    const response= pm.response.json()\r",
											"    let titleToFind= pm.collectionVariables.get(\"bookTitle\")\r",
											"/*\r",
											"prepare query varaible to comparison    \r",
											"use lodash library  deburr method  and regex to discard accents and ponctuation  in query variable \r",
											"transforme queyr varaible to upper case\r",
											"*/\r",
											"    \r",
											"function includesString(strTotest, testedStr){\r",
											"    const regex = /[!\"#$%&'()*+,-./:;<=>?@[\\]^_`{|}~’]/g;\r",
											"    // let titi=strTotest.normalize('NFD').replace(/[\\u0300-\\u036f]/g, \"\").toLowerCase()\r",
											"    // let toto=testedStr.normalize('NFD').replace(/[\\u0300-\\u036f]/g, \"\").toLowerCase()\r",
											"\r",
											"    strTotest=_.deburr(strTotest).toLowerCase().replace(regex, ' ');\r",
											"    testedStr=_.deburr(testedStr).toLowerCase().replace(regex, ' ');\r",
											"     console.log(\"var_1\", strTotest,\"var_2\",testedStr, \"test de validation\",strTotest.includes(testedStr))\r",
											"    return strTotest.includes(testedStr)\r",
											"}\r",
											"\r",
											"  \r",
											"   \r",
											"  \r",
											"\r",
											"/**\r",
											" * \r",
											" * function that returns an array of boolean if title of item in response ocntains the variable\r",
											" *  */    \r",
											"    const booleanTitleArray= response.items.map((item)=> includesString(item.volumeInfo.title,titleToFind))\r",
											" \r",
											"/**\r",
											" * function that returns an array of boolean if subtitle of item in response ocntains the variable\r",
											" * if subtitle is undefined return false \r",
											" */\r",
											"    \r",
											"    const booleanSubtitleArray= response.items.map((item)=>item.volumeInfo.subtitle)\r",
											"    .map((subtitle)=>typeof subtitle === \"undefined\" ? false : subtitle)\r",
											"    .map((subtitle)=>includesString(subtitle, titleToFind))\r",
											"\r",
											"   \r",
											"/**\r",
											" * \r",
											" * function that adds merges the two arrays of boolean values with the OR operator\r",
											" * will be use in the assertion\r",
											" */\r",
											"    const mergeArrays= ()=>{\r",
											"        const mergedArr=[booleanTitleArray, booleanSubtitleArray]\r",
											"    \r",
											"        const arrayToTest=mergedArr.reduce((acc, item)=>{\r",
											"            item.forEach((bool, index)=>{\r",
											"                acc[index]=acc[index] || bool\r",
											"            })\r",
											"            return acc\r",
											"        },[])\r",
											"        return arrayToTest\r",
											"    }\r",
											" \r",
											"    pm.expect(mergeArrays().includes(false)).to.be.eq(false)\r",
											"\r",
											"})\r",
